{
  "name": "express-standard-http-responses",
  "version": "2.0.0",
  "description": "Helper package used to manage http responses for express",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "test": "yarn nodemon"
  },
  "keywords": [
    "express",
    "responses",
    "response",
    "http",
    "nodejs",
    "typescript",
    "service"
  ],
  "author": "Arthur LE RAY",
  "license": "MIT",
  "devDependencies": {
    "express": "^4.18.1",
    "nodemon": "^2.0.16",
    "ts-node": "^10.7.0",
    "tsc": "^2.0.4",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@types/express": "^4.17.13"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "interfaces/**",
    "LICENSE"
  ],
  "nodemonConfig": {
    "watch": [
      "tests"
    ],
    "ext": "ts",
    "exec": "tsc && ts-node tests/test.ts"
  }
}
